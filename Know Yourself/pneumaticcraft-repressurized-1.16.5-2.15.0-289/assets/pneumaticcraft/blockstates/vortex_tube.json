{
  "multipart": [
    { "when": {"facing": "down" },  "apply": { "model": "pneumaticcraft:block/vortex_tube", "x": 270 } },
    { "when": {"facing": "up" },    "apply": { "model": "pneumaticcraft:block/vortex_tube", "x": 90} },
    { "when": {"facing": "north" }, "apply": { "model": "pneumaticcraft:block/vortex_tube", "y": 180} },
    { "when": {"facing": "south" }, "apply": { "model": "pneumaticcraft:block/vortex_tube" } },
    { "when": {"facing": "east" },  "apply": { "model": "pneumaticcraft:block/vortex_tube", "y": 270 } },
    { "when": {"facing": "west" },  "apply": { "model": "pneumaticcraft:block/vortex_tube", "y": 90 } },
    { "when": {"down": true },  "apply": { "model": "pneumaticcraft:block/pressure_tube_connected", "x": 180 }},
    { "when": {"up": true },    "apply": { "model": "pneumaticcraft:block/pressure_tube_connected" }},
    { "when": {"north": true }, "apply": { "model": "pneumaticcraft:block/pressure_tube_connected", "x": 90 }},
    { "when": {"south": true }, "apply": { "model": "pneumaticcraft:block/pressure_tube_connected", "x": 90, "y": 180 }},
    { "when": {"west": true },  "apply": { "model": "pneumaticcraft:block/pressure_tube_connected", "x": 90, "y": 270 }},
    { "when": {"east": true },  "apply": { "model": "pneumaticcraft:block/pressure_tube_connected", "x": 90, "y": 90 }}
  ]
}