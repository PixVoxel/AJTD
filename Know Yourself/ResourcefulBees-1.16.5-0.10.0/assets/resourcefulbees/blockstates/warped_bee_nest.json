{
  "multipart": [
    {
      "when": { "facing": "north" },
      "apply": { "model": "resourcefulbees:block/warped_bee_nest", "y": 0 }
    },
    {
      "when": { "honey_level": 5, "facing": "north"},
      "apply": { "model": "resourcefulbees:block/warped_bee_nest_honey", "y": 0 }
    },
    {
      "when": { "facing": "south" },
      "apply": { "model": "resourcefulbees:block/warped_bee_nest", "y": 180 }
    },
    {
      "when": { "honey_level": 5, "facing": "south"},
      "apply": { "model": "resourcefulbees:block/warped_bee_nest_honey", "y": 180 }
    },
    {
      "when": { "facing": "east" },
      "apply": { "model": "resourcefulbees:block/warped_bee_nest", "y": 90 }
    },
    {
      "when": { "honey_level": 5, "facing": "east"},
      "apply": { "model": "resourcefulbees:block/warped_bee_nest_honey", "y": 90 }
    },
    {
      "when": { "facing": "west" },
      "apply": { "model": "resourcefulbees:block/warped_bee_nest", "y": 270 }
    },
    {
      "when": { "honey_level": 5, "facing": "west"},
      "apply": { "model": "resourcefulbees:block/warped_bee_nest_honey", "y": 270 }
    },
    {
      "when": { "tier" : 0 },
      "apply": { "model": "resourcefulbees:block/nest_tier_0" }
    },
    {
      "when": { "tier" : 1 },
      "apply": { "model": "resourcefulbees:block/nest_tier_1" }
    },
    {
      "when": { "tier" : 2 },
      "apply": { "model": "resourcefulbees:block/nest_tier_2" }
    },
    {
      "when": { "tier" : 3 },
      "apply": { "model": "resourcefulbees:block/nest_tier_3" }
    },
    {
      "when": { "tier" : 4 },
      "apply": { "model": "resourcefulbees:block/nest_tier_4" }
    }
  ]
}